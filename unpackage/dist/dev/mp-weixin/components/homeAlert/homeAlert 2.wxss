.homeAlert {
  width: 100%;
  position: fixed;
  top: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.7);
  height: 100vh;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  z-index: 999;
  /* 播放头像 */
}
.homeAlert .star_box {
    width: 520rpx;
    height: 340rpx;
    background: #d44040;
    border-radius: 20rpx;
    position: relative;
}
.homeAlert .star_box .go_login_box {
      width: 560rpx;
      height: 300rpx;
      padding: 50rpx 0 30rpx 0;
      background: #ff6666;
      border-radius: 20rpx;
      position: absolute;
      left: -20rpx;
      top: 48rpx;
}
.homeAlert .star_box .go_login_box .color_fff {
        font-size: 32rpx;
        padding: 15rpx 0;
}
.homeAlert .star_box .go_login_box .fontsize32 {
        font-size: 32rpx;
}
.homeAlert .star_box .go_login_box .color_all {
        width: 296rpx;
        height: 100rpx;
        background: white;
        border-radius: 50rpx;
        line-height: 100rpx;
        text-align: center;
        margin-top: 60rpx;
}
.homeAlert .star_box .big_block {
      height: 231rpx;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
          -ms-flex-pack: center;
              justify-content: center;
      position: relative;
}
.homeAlert .star_box .big_block .star_img {
        width: 231rpx;
        height: 231rpx;
        background: #d8d8d8;
        border-radius: 50%;
        border: 60rpx solid black;
        margin-top: -170rpx;
}
.homeAlert .star_box .big_block .center_block {
        width: 65rpx;
        height: 65rpx;
        background: rgba(0, 0, 0, 0.5);
        border-radius: 50%;
        position: absolute;
        top: 463rpx;
        -webkit-box-pack: center;
        -webkit-justify-content: center;
            -ms-flex-pack: center;
                justify-content: center;
        top: 0;
}
.homeAlert .star_box .big_block .center_block .bg_fff {
          width: 30rpx;
          height: 30rpx;
          border-radius: 50%;
}

